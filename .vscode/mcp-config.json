{
    "mcp-client": {
        "servers": {
            "MaxMSPMCP": {
                "command": "uv",
                "args": [
                    "run",
                    "python",
                    "server.py"
                ],
                "cwd": "${workspaceFolder}",
                "env": {
                    "SOCKETIO_SERVER_URL": "http://127.0.0.1",
                    "SOCKETIO_SERVER_PORT": "5002"
                }
            }
        }
    }
}